<% content_for :page_title do %>
  <%= t("editing_banner") %>
<% end %>

<%= render "spree/shared/error_messages", :target => @banner %>

<%= form_for [:admin, @banner], url: admin_banner_url, method: :put do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <div data-hook="admin_banner_new_form_buttons">
    <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
  </div>
<% end %>